{
	"manifest_version": 2,
	"name": "Widen â€” Get More Instagram Followers",
	"version": "1.0",
	"description": "Widen helps you follow all of someone's followers, followings, or likers. grow your Instagram today!",
	"permissions": [
		"tabs",
		"https://www.instagram.com/*"
	],
	"browser_action": {
		"default_popup": "login/index.html",
		"icons": {
			"16": "./images/16x16.png",
			"48": "./images/48x48.png",
			"128": "./images/128x128.png"
		}
	},
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"content_security_policy": "script-src 'self' https://cdn.firebase.com https://apis.google.com https://www.gstatic.com; object-src 'self'"
}